		"security:outdated": "pnpm outdated",
		"security:lint": "eslint . --config .eslintrc.json",
		"security:fix": "eslint . --config .eslintrc.json --fix",
		"security:check": "pnpm run security:audit && pnpm run security:outdated && pnpm run security:lint",
