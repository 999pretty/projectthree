		"test:e2e": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_ENV=test SENTRY_DISABLED=true dotenv -c -- pnpm test:e2e:build && pnpm test:e2e:start",
		"analyze": "cross-env ANALYZE=true SENTRY_DISABLED=true BETTER_AUTH_SECRET=dummy GOOGLE_CLIENT_ID=dummy GOOGLE_CLIENT_SECRET=dummy GITHUB_CLIENT_ID=dummy GITHUB_CLIENT_SECRET=dummy next build",
		"analyze:server": "cross-env ANALYZE=true BUNDLE_ANALYZE=server SENTRY_DISABLED=true BETTER_AUTH_SECRET=dummy GOOGLE_CLIENT_ID=dummy GOOGLE_CLIENT_SECRET=dummy GITHUB_CLIENT_ID=dummy GITHUB_CLIENT_SECRET=dummy next build",
		"analyze:browser": "cross-env ANALYZE=true BUNDLE_ANALYZE=browser SENTRY_DISABLED=true BETTER_AUTH_SECRET=dummy GOOGLE_CLIENT_ID=dummy GOOGLE_CLIENT_SECRET=dummy GITHUB_CLIENT_ID=dummy GITHUB_CLIENT_SECRET=dummy next build",
